<%= form_for Post.new do |f| %>
  <div class="form-group">
    <%= f.text_field :description %>
  </div>
  <div class="form-group">
    <%= f.file_field :image %>
  </div>
  <div class="form-group">
    <%= f.text_field :user_id, value: current_user.id, class: "d-none" %>
  </div>
  <br>
  <div calls="text-center">
    <%= f.submit 'Creat Post', class "btn btn-primary" %>
  </div>
<% end %>